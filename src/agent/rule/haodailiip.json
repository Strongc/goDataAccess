{
    "task_name": "haodailiip",
    "base_url": "http://www.haodailiip.com/",
    "max_depth": 5,
    "start_urls": [
        "http://www.haodailiip.com/"
    ],
    "item_rule": {
        "scope_rule": "(?s)<tr style=\"font-size.*?>(.*?)</tr>",
        "kv_rule": {
            "ip": "(?s)<td>.*?(\\d+\\.\\d+\\.\\d+\\.\\d+).*?</td>",
            "port": "(?s)<td>.*?(\\d+).*?</td>"
        },
        "trim_func": "trim_html_tags"
    },
    "merge": false,
    "concurrency": 200
}
