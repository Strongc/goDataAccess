{
    "task_name": "xicidaili",
    "base_url": "http://www.xicidaili.com",
    "start_urls": [
        "http://www.xicidaili.com/nn/",
        "http://www.xicidaili.com/nt/",
        "http://www.xicidaili.com/wn/",
        "http://www.xicidaili.com/wt/"
    ],
    "item_rule": {
        "scope_rule": "(?s)<tr .*?>.*?<td>.*?</tr>",
        "kv_rule": {
            "ip": "(?s)<td>(\\d+\\.\\d+\\.\\d+\\.\\d+)</td>",
            "port": "(?s)<td>(\\d+)</td>"
        },
        "trim_func": "trim_html_tags"
    },
    "request_rule": {
        "scope_rule": "(?s)<a class=\"next_page\" rel=\"next\" href=\"(.*?)\">",
        "kv_rule": {
            "url1": "href=\"(.*?)\""
        },
        "trim_func": "trim_html_tags"
    },
    "merge": false,
    "config": {
        "concurrency": 10,
        "succ": "xicidaili",
        "max_retry_times": 5,
        "user_agent": "Mozilla/5.0 (X11; Linux i686) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/41.0.2272.35 Safari/537.36"
    }
}
